,qBAAA,aAAA,GAAA,KAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,qBAAA,SAAA,GAAA,KAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,eAAA;;AATA,qBAUA,KAAA,aAVA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAWA,KAAA,OAAA,CAAA,IAAA,CAAA;AAAA,kBAAA,IAAA,EAAA;AAAA,iBAAA,CAXA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AADA,GAZA;AA4BA,EAAA,KAAA,EAAA;AACA,IAAA,aADA,yBACA,GADA,EACA;AAAA;;AACA,UAAA,CAAA,GAAA,EAAA;AACA,aAAA,KAAA,GAAA,IAAA;AACA,QAAA,UAAA,CAAA,YAAA;AAAA,UAAA,KAAA,CAAA,KAAA,GAAA,KAAA;AAAA,SAAA,EAAA,KAAA,CAAA;AACA,aAAA,aAAA,GAAA,IAAA;AACA;AACA;AAPA,GA5BA;AAqCA,EAAA,OArCA,qBAqCA,CACA;AAtCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"Login\">\r\n    <v-main>\r\n      <v-container fluid fill-height>\r\n        <v-layout align-center justify-center>\r\n          <v-flex xs12 sm8 md4>\r\n            <v-alert v-model=\"alert\" align=\"center\" :value=\"alert\" dismissible color=\"red\" border=\"left\" elevation=\"5\" colored-border icon=\"mdi-cancel\"><strong>{{this.alertText}}</strong></v-alert>\r\n            <v-card class=\"elevation-12\">\r\n              <v-toolbar dark color=\"primary\">\r\n                <v-toolbar-title>Welcome to Bryant-Insurance</v-toolbar-title>\r\n              </v-toolbar>\r\n              <v-card-text>\r\n                <v-form>\r\n                  <v-text-field\r\n                      v-model=\"username\"\r\n                      id=\"username\"\r\n                      name=\"username\"\r\n                      label=\"Username\"\r\n                      type=\"text\"\r\n                  ></v-text-field>\r\n                  <v-text-field\r\n                      v-model=\"password\"\r\n                      id=\"password\"\r\n                      name=\"password\"\r\n                      label=\"Password\"\r\n                      type=\"password\"\r\n                  ></v-text-field>\r\n                </v-form>\r\n              </v-card-text>\r\n              <v-card-actions>\r\n                <v-spacer> </v-spacer>\r\n<!--                TODO add actions and send to api-->\r\n                <v-btn color=\"primary\" v-on:click=\"login\">Login</v-btn>\r\n                <v-dialog v-model=\"dialog\" persistent max-width=\"600px\" >\r\n                  <template v-slot:activator=\"{ on, attrs }\">\r\n                    <v-btn color=\"primary\" dark v-bind=\"attrs\" v-on=\"on\" class=\"mx-2\">\r\n                      Create Account\r\n                    </v-btn>\r\n                  </template>\r\n                  <v-card>\r\n                    <v-card-title>\r\n                      <span class=\"text-h5\">Create User</span>\r\n                    </v-card-title>\r\n                    <v-card-text>\r\n                      <v-container>\r\n                        <v-row>\r\n                          <v-col cols=\"12\" sm=\"6\" md=\"4\" >\r\n                            <v-text-field\r\n                                label=\"Legal first name*\"\r\n                                required\r\n                            ></v-text-field>\r\n                          </v-col>\r\n                          <v-col cols=\"12\">\r\n                            <v-text-field\r\n                                label=\"Email*\"\r\n                                required\r\n                            ></v-text-field>\r\n                          </v-col>\r\n                          <v-col cols=\"12\">\r\n                            <v-text-field\r\n                                label=\"Password*\"\r\n                                type=\"password\"\r\n                                required\r\n                            ></v-text-field>\r\n                          </v-col>\r\n                        </v-row>\r\n                      </v-container>\r\n                      <small>*indicates required field</small>\r\n                    </v-card-text>\r\n                    <v-card-actions>\r\n                      <v-spacer></v-spacer>\r\n                      <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">Close</v-btn>\r\n                      <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">Save</v-btn>\r\n                    </v-card-actions>\r\n                  </v-card>\r\n                </v-dialog>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-flex>\r\n        </v-layout>\r\n      </v-container>\r\n    </v-main>\r\n  </div>\r\n</template>\r\n\r\n\r\n\r\n<script>\r\nimport api from \"@/service/apiService\";\r\nexport default {\r\n  name: \"Author\",\r\n  data() {\r\n    return {\r\n      username: \"\",\r\n      password: \"\",\r\n      alert: false,\r\n      requestStatus: null,\r\n      alertText: \"\",\r\n      dialog: false,\r\n    };\r\n  },\r\n  methods: {\r\n    login: async function() {\r\n      this.dialog = false;\r\n      let requestData = new FormData();\r\n      requestData.append(\"username\",this.username);\r\n      requestData.append(\"password\",this.password);\r\n      let requestResponse = await api.login(requestData);\r\n      this.response = requestResponse;\r\n      this.requestStatus = this.response.data.success;\r\n      this.alertText = this.response.data.message;\r\n      console.log(requestResponse);\r\n      if(this.requestStatus){\r\n        await this.$router.push({name: \"Home\"}); // original was this.$router.push({name: \"Home\"});\r\n      }\r\n    },\r\n  },\r\n  watch: {\r\n    requestStatus(val){\r\n      if(!val){\r\n        this.alert = true\r\n        setTimeout(()=>{this.alert=false},15000)\r\n        this.requestStatus = null\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/components"}]}