ANA;AAcA,IAAA,aAAA;AAAA,mFAAA,kBAAA,EAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,GAAA,CAAA,aAAA,CAAA,EAAA,CADA;;AAAA;AACA,gBAAA,QADA;AAEA,gBAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,qBAAA,aAAA,GAAA,KAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,OAdA;AAmBA,IAAA,UAAA;AAAA,gFAAA,kBAAA,EAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,EAAA;AACA,gBAAA,WAFA,GAEA;AACA,kBAAA,IAAA,EAAA,KAAA;AADA,iBAFA;AAKA,gBAAA,OAAA,CAAA,GAAA,CAAA,WAAA,CAAA,IAAA;AALA;AAAA,uBAMA,GAAA,CAAA,UAAA,CAAA,WAAA,EAAA,KAAA,UAAA,CAAA,EAAA,CANA;;AAAA;AAMA,gBAAA,QANA;AAOA,gBAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,qBAAA,YAAA,GAAA,EAAA;AACA,qBAAA,aAAA,GAAA,KAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,OAnBA;AA8BA,IAAA,UA9BA,sBA8BA,MA9BA,EA8BA;AACA,WAAA,WAAA,GAAA,KAAA,OAAA,CAAA,OAAA,CAAA,MAAA,CAAA;AACA,WAAA,UAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,MAAA,CAAA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,KAlCA;AAmCA,IAAA,mBAnCA,+BAmCA,IAnCA,EAmCA;AACA,WAAA,WAAA,GAAA,KAAA,OAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,WAAA,UAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,IAAA,CAAA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,KAvCA;AAwCA,IAAA,YAAA;AAAA,kFAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,GAAA,CAAA,YAAA,CAAA,KAAA,UAAA,CAAA,EAAA,CADA;;AAAA;AACA,gBAAA,QADA;AAEA,gBAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,MAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,OAxCA;AA6CA,IAAA,KA7CA,mBA6CA;AAAA;;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,KAAA,CAAA,WAAA,CAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA;AAIA,KAnDA;AAoDA,IAAA,WApDA,yBAoDA;AAAA;;AACA,WAAA,YAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,MAAA,CAAA,WAAA,CAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA;AAIA,KA1DA;AA2DA,IAAA,IAAA;AAAA,0EAAA,kBAAA,EAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBAEA,KAAA,WAAA,GAAA,CAAA,CAFA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAGA,GAAA,CAAA,YAAA,CAAA,KAAA,UAAA,EAAA,EAAA,CAHA;;AAAA;AAGA,gBAAA,QAHA;AAIA,gBAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,MAAA;AALA;AAAA;;AAAA;AAAA;AAAA,uBAOA,GAAA,CAAA,YAAA,CAAA,KAAA,UAAA,CAPA;;AAAA;AAOA,gBAAA,SAPA;AAQA,gBAAA,OAAA,CAAA,GAAA,CAAA,SAAA;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,MAAA;;AATA;AAWA,qBAAA,KAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AA3DA,GA7DA;AAsIA,EAAA,OAtIA,qBAsIA;AACA,SAAA,WAAA;AACA,GAxIA;AAyIA,EAAA,QAAA,EAAA;AACA,IAAA,SADA,uBACA;AACA,aAAA,KAAA,WAAA,KAAA,CAAA,CAAA,GAAA,mBAAA,GAAA,aAAA;AACA;AAHA,GAzIA;AA8IA,EAAA,KAAA,EAAA;AACA,IAAA,MADA,kBACA,GADA,EACA;AACA,MAAA,GAAA,IAAA,KAAA,KAAA,EAAA;AACA,KAHA;AAIA,IAAA,YAJA,wBAIA,GAJA,EAIA;AACA,MAAA,GAAA,IAAA,KAAA,WAAA,EAAA;AACA,KANA;AAOA,IAAA,aAPA,yBAOA,GAPA,EAOA;AACA,MAAA,GAAA,IAAA,KAAA,WAAA,EAAA;AACA;AATA;AA9IA,CAAA","sourcesContent":["<template class=\"max-\">\r\n<v-app>\r\n  <v-data-table :headers=\"headers\" :items=\"clients\" class=\"elevation-1\" height=\"550px\" :search=\"search\">\r\n    <template v-slot:top>\r\n      <v-toolbar flat>\r\n        <v-toolbar-title>CLIENTS LIST</v-toolbar-title>\r\n        <v-divider class=\"mx-4\" inset vertical></v-divider>\r\n        <v-text-field v-model=\"search\" append-icon=\"mdi-magnify\" label=\"Search\" single-line hide-details></v-text-field>\r\n        <v-spacer></v-spacer>\r\n        <v-dialog persistent no-click-animation v-model=\"dialog\" max-width=\"1000px\" >\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-btn color=\"primary\" dark class=\"mb-2\" v-bind=\"attrs\" v-on=\"on\">New Client</v-btn>\r\n          </template>\r\n          <v-card>\r\n            <v-card-title>\r\n              <span class=\"text-h5\">{{ formTitle }}</span>\r\n            </v-card-title>\r\n            <v-card-text>\r\n              <v-container>\r\n                <v-row>\r\n                  <v-col cols=\"12\" sm=\"6\" >\r\n                    <v-text-field v-model=\"editedItem.firstname\" label=\"Firstname\"></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\" >\r\n                    <v-text-field v-model=\"editedItem.lastname\" label=\"Lastname\" ></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\">\r\n                    <v-text-field v-model=\"editedItem.email\" label=\"Email\"></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\">\r\n                    <v-text-field v-model=\"editedItem.phoneNumber\" label=\"Phone Number\"></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\" >\r\n                    <v-menu v-model=\"menu0\" :close-on-content-click=\"false\" :nudge-right=\"40\" transition=\"scale-transition\" offset-y min-width=\"auto\">\r\n                      <template v-slot:activator=\"{ on, attrs }\">\r\n                        <v-text-field v-model=\"editedItem.dateSold\" label=\"Date Sold\" prepend-icon=\"mdi-calendar\" readonly v-bind=\"attrs\" v-on=\"on\"></v-text-field>\r\n                      </template>\r\n                      <v-date-picker v-model=\"editedItem.dateSold\" @input=\"menu0 = false\"></v-date-picker>\r\n                    </v-menu>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\" >\r\n                    <v-menu v-model=\"menu1\" :close-on-content-click=\"false\" :nudge-right=\"40\" transition=\"scale-transition\" offset-y min-width=\"auto\">\r\n                      <template v-slot:activator=\"{ on, attrs }\">\r\n                        <v-text-field v-model=\"editedItem.quoteDate\" label=\"Quote Date\" prepend-icon=\"mdi-calendar\" readonly v-bind=\"attrs\" v-on=\"on\"></v-text-field>\r\n                      </template>\r\n                      <v-date-picker v-model=\"editedItem.quoteDate\" @input=\"menu1 = false\"></v-date-picker>\r\n                    </v-menu>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\" >\r\n                    <v-menu v-model=\"menu2\" :close-on-content-click=\"false\" :nudge-right=\"40\" transition=\"scale-transition\" offset-y min-width=\"auto\">\r\n                      <template v-slot:activator=\"{ on, attrs }\">\r\n                        <v-text-field v-model=\"editedItem.expiryDate\" label=\"Expiry Date\" prepend-icon=\"mdi-calendar\" readonly v-bind=\"attrs\" v-on=\"on\"></v-text-field>\r\n                      </template>\r\n                      <v-date-picker v-model=\"editedItem.expiryDate\" @input=\"menu2 = false\"></v-date-picker>\r\n                    </v-menu>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\" >\r\n                    <v-menu v-model=\"menu3\" :close-on-content-click=\"false\" :nudge-right=\"40\" transition=\"scale-transition\" offset-y min-width=\"auto\">\r\n                      <template v-slot:activator=\"{ on, attrs }\">\r\n                        <v-text-field v-model=\"editedItem.latestContactDate\" label=\"Latest Contact Date\" prepend-icon=\"mdi-calendar\" readonly v-bind=\"attrs\" v-on=\"on\"></v-text-field>\r\n                      </template>\r\n                      <v-date-picker v-model=\"editedItem.latestContactDate\" @input=\"menu3 = false\"></v-date-picker>\r\n                    </v-menu>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\" >\r\n                    <v-text-field v-model=\"editedItem.quoteStatus\" label=\"Quote Status\"></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\" >\r\n                    <v-text-field v-model=\"editedItem.commissionAmount\" label=\"Commission Amount\"></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\" >\r\n                    <v-text-field v-model=\"editedItem.referral\" label=\"Referral\"></v-text-field>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-container>\r\n            </v-card-text>\r\n<!--            NEW CLIENT POP UP CARD-->\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n              <v-btn color=\"blue darken-1\" text @click=\"close\">Cancel</v-btn>\r\n              <v-btn color=\"blue darken-1\" text @click=\"save(editedItem.id)\">Save</v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n<!--        CARRIER DIALOG-->\r\n        <v-dialog v-model=\"dialogCarrier\" max-width=\"500px\">\r\n          <v-card>\r\n            <v-card-title v-model=\"editedItem\">List of {{editedItem.firstname}}'s Carriers</v-card-title>\r\n            <v-container>\r\n              <v-row>\r\n                <v-col cols=\"12\" sm=\"6\" >\r\n                  <v-text-field v-model=\"toAddCarrier\" label=\"Carrier Type\"></v-text-field>\r\n                  <v-btn color=\"primary\" dark class=\"mb-2\" tile @click=\"addCarrier(editedItem.id)\">Add Carrier</v-btn>\r\n                </v-col>\r\n                  <v-col sm=\"6\" >\r\n                  <v-simple-table dark>\r\n                    <thead>\r\n                    </thead>\r\n                    <template v-slot:default>\r\n                      <tbody>\r\n                      <tr v-for=\"c in carrier\" :key=\"c.id\">\r\n                        <td>{{ c.type }}</td>\r\n                        <td><v-icon right title=\"Delete Carrier\" @click=\"deleteCarrier(c.id)\">mdi-delete</v-icon></td>\r\n                      </tr>\r\n                      </tbody>\r\n                    </template>\r\n                  </v-simple-table>\r\n                </v-col>\r\n              </v-row>\r\n            </v-container>\r\n          </v-card>\r\n        </v-dialog>\r\n<!--        DELETE DIALOG-->\r\n        <v-dialog v-model=\"dialogDelete\" max-width=\"600px\">\r\n          <v-card>\r\n            <v-card-title v-model=\"editedItem\" class=\"text-h5 justify-center\">Are you sure you want to remove client: {{editedItem.firstname}}?</v-card-title>\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n<!--              TODO: SEND REQUEST HERE BROTHER-->\r\n              <v-btn color=\"blue darken-1\" text @click=\"closeDelete\">Cancel</v-btn>\r\n              <v-btn color=\"blue darken-1\" text @click=\"deleteClient\">OK</v-btn>\r\n              <v-spacer></v-spacer>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n      </v-toolbar>\r\n    </template>\r\n    <template v-slot:item.actions=\"{ item }\">\r\n      <v-icon small class=\"mr-2\" @click=\"setCarrier(item)\" title=\"View Client Carrier\">mdi-account</v-icon>\r\n      <v-icon small class=\"mr-2\" @click=\"editClient(item)\" title=\"Edit Client\">mdi-pencil</v-icon>\r\n      <v-icon small @click=\"confirmDeleteClient(item)\" title=\"Delete Client\">mdi-delete</v-icon>\r\n    </template>\r\n  </v-data-table>\r\n</v-app>\r\n</template>\r\n\r\n<script>\r\nimport api from \"@/service/apiService\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      date: (new Date(Date.now() - (new Date()).getTimezoneOffset() * 60000)).toISOString(),\r\n      menu0: false,\r\n      menu1: false,\r\n      menu2: false,\r\n      menu3: false,\r\n      dialog: false,\r\n      dialogDelete: false,\r\n      dialogCarrier: false,\r\n      clients: [],\r\n      search: \"\",\r\n      toAddCarrier: \"\",\r\n      editedIndex: -1,\r\n      carrier: [],\r\n      headers: [\r\n        {text: 'Client Id', value: 'id'},\r\n        {text: 'Firstname', value: 'firstname'},\r\n        {text: 'Lastname', value: 'lastname'},\r\n        {text: 'Email', value: 'email'},\r\n        {text: 'Phone Number', value: 'phoneNumber'},\r\n        {text: 'Quote Date', value: 'quoteDate'},\r\n        {text: 'Expiry Date', value: 'expiryDate'},\r\n        {text: 'Latest Contact Date', value: 'latestContactDate'},\r\n        {text: 'Quote Status', value: 'quoteStatus'},\r\n        {text: 'Commission Amount', value: 'commissionAmount'},\r\n        {text: 'Actions', value: 'actions', sortable: false},\r\n      ],\r\n      // TODO: FIX THIS LATER\r\n      editedItem: {\r\n        id: '',\r\n        firstname: '',\r\n        lastname: '',\r\n        email: '',\r\n        phoneNumber: '',\r\n        dateSold: '',\r\n        quoteDate: '',\r\n        expiryDate: '',\r\n        latestContactDate: '',\r\n        quoteStatus: '',\r\n        commissionAmount: '',\r\n        referral: '',\r\n      },\r\n      defaultItem: {\r\n        id: '',\r\n        firstname: '',\r\n        lastname: '',\r\n        email: '',\r\n        phoneNumber: '',\r\n        dateSold: '',\r\n        quoteDate: '',\r\n        expiryDate: '',\r\n        latestContactDate: '',\r\n        quoteStatus: '',\r\n        commissionAmount: '',\r\n        referral: '',\r\n      },\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    readClients: async function () {\r\n      const data = await api.readClient();\r\n      this.clients = data;\r\n      console.log(data)\r\n    },\r\n    setCarrier: async function(client){\r\n      this.editedItem = Object.assign({},client)\r\n      let carrier = await api.getCarrier(this.editedItem.id)\r\n      console.log(this.editedItem);\r\n      this.carrier = carrier;\r\n      console.log(carrier);\r\n      this.dialogCarrier = true;\r\n    },\r\n    deleteCarrier: async function(id){\r\n      let response = await api.deleteCarrier(id)\r\n      console.log(response)\r\n      this.dialogCarrier = false;\r\n    },\r\n    addCarrier: async function(id){\r\n      console.log(id)\r\n      const requestData = {\r\n        type: this.toAddCarrier,\r\n      }\r\n      console.log(requestData.data)\r\n      let response = await api.addCarrier(requestData,this.editedItem.id)\r\n      console.log(response)\r\n      this.toAddCarrier = \"\";\r\n      this.dialogCarrier = false;\r\n    },\r\n    editClient(client) {\r\n      this.editedIndex = this.clients.indexOf(client)\r\n      this.editedItem = Object.assign({}, client)\r\n      this.dialog = true\r\n    },\r\n    confirmDeleteClient(item){\r\n      this.editedIndex = this.clients.indexOf(item)\r\n      this.editedItem = Object.assign({}, item)\r\n      this.dialogDelete = true\r\n    },\r\n    deleteClient: async function() {\r\n      let response = await api.deleteClient(this.editedItem.id)\r\n      console.log(response)\r\n      window.location.reload();\r\n    },\r\n    close () {\r\n      this.dialog = false\r\n      this.$nextTick(() => {\r\n        this.editedItem = Object.assign({}, this.defaultItem)\r\n        this.editedIndex = -1\r\n      })\r\n    },\r\n    closeDelete () {\r\n      this.dialogDelete = false\r\n      this.$nextTick(() => {\r\n        this.editedItem = Object.assign({}, this.defaultItem)\r\n        this.editedIndex = -1\r\n      })\r\n    },\r\n    save: async function(id) {\r\n      // edit the user\r\n      if (this.editedIndex > -1) {\r\n        let response = await api.updateClient(this.editedItem,id)\r\n        console.log(response)\r\n        window.location.reload();\r\n       } else { // add new user\r\n        let response = await api.createClient(this.editedItem)\r\n        console.log(response)\r\n        window.location.reload();\r\n      }\r\n      this.close()\r\n    },\r\n  },\r\n  mounted() {\r\n    this.readClients();\r\n  },\r\n  computed: {\r\n    formTitle () {\r\n      return this.editedIndex === -1 ? 'Create new client' : 'Edit client'\r\n    },\r\n  },\r\n  watch: {\r\n    dialog (val) {\r\n      val || this.close()\r\n    },\r\n    dialogDelete (val) {\r\n      val || this.closeDelete()\r\n    },\r\n    dialogCarrier (val){\r\n      val || this.closeDelete()\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}