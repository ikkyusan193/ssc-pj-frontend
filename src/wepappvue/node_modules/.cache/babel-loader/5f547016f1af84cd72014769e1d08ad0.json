A,EAAA,EAAA,EADA;AAEA,QAAA,QAAA,EAAA,EAFA;AAGA,QAAA,IAAA,EAAA;AAHA;AAlBA,KAAA;AAwBA,GA1BA;AA4BA,EAAA,OAAA,EAAA;AACA,IAAA,SAAA;AAAA,+EAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,GAAA,CAAA,QAAA,EADA;;AAAA;AACA,gBAAA,IADA;AAEA,qBAAA,KAAA,GAAA,IAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,IAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,OADA;AAMA,IAAA,QANA,oBAMA,IANA,EAMA;AACA,WAAA,WAAA,GAAA,KAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,WAAA,UAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,IAAA,CAAA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,KAVA;AAWA,IAAA,iBAXA,6BAWA,IAXA,EAWA;AACA,WAAA,WAAA,GAAA,KAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,WAAA,UAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,IAAA,CAAA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,KAfA;AAgBA,IAAA,UAAA;AAAA,gFAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,GAAA,CAAA,UAAA,CAAA,KAAA,UAAA,CAAA,EAAA,CADA;;AAAA;AACA,gBAAA,QADA;AAEA,gBAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,MAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,OAhBA;AAqBA,IAAA,KArBA,mBAqBA;AAAA;;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,KAAA,CAAA,WAAA,CAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA;AAIA,KA3BA;AA4BA,IAAA,WA5BA,yBA4BA;AAAA;;AACA,WAAA,YAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,MAAA,CAAA,WAAA,CAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA;AAIA,KAlCA;AAmCA,IAAA,IAAA;AAAA,0EAAA,kBAAA,EAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,KAAA,WAAA,GAAA,CAAA,CADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,GAAA,CAAA,UAAA,CAAA,KAAA,UAAA,EAAA,EAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,MAAA;AAJA;AAAA;;AAAA;AAAA;AAAA,uBAMA,GAAA,CAAA,UAAA,CAAA,KAAA,UAAA,CANA;;AAAA;AAMA,gBAAA,SANA;AAOA,gBAAA,OAAA,CAAA,GAAA,CAAA,SAAA;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,MAAA;;AARA;AAUA,qBAAA,KAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAnCA,GA5BA;AA4EA,EAAA,OA5EA,qBA4EA;AACA,SAAA,SAAA;AACA,GA9EA;AA+EA,EAAA,QAAA,EAAA;AACA,IAAA,SADA,uBACA;AACA,aAAA,KAAA,WAAA,KAAA,CAAA,CAAA,GAAA,iBAAA,GAAA,WAAA;AACA;AAHA,GA/EA;AAoFA,EAAA,KAAA,EAAA;AACA,IAAA,MADA,kBACA,GADA,EACA;AACA,MAAA,GAAA,IAAA,KAAA,KAAA,EAAA;AACA,KAHA;AAIA,IAAA,YAJA,wBAIA,GAJA,EAIA;AACA,MAAA,GAAA,IAAA,KAAA,WAAA,EAAA;AACA;AANA;AApFA,CAAA","sourcesContent":["<template class=\"max-\">\r\n  <v-app>\r\n    <v-data-table :headers=\"headers\" :items=\"users\" class=\"elevation-1\" height=\"550px\" :search=\"search\">\r\n      <template v-slot:top>\r\n        <v-toolbar flat>\r\n          <v-toolbar-title>USERS LIST</v-toolbar-title>\r\n          <v-divider class=\"mx-4\" inset vertical></v-divider>\r\n          <v-text-field v-model=\"search\" append-icon=\"mdi-magnify\" label=\"Search\" single-line hide-details></v-text-field>\r\n          <v-spacer></v-spacer>\r\n          <v-dialog persistent no-click-animation v-model=\"dialog\" max-width=\"1000px\" >\r\n            <template v-slot:activator=\"{ on, attrs }\">\r\n              <v-btn color=\"primary\" dark class=\"mb-2\" v-bind=\"attrs\" v-on=\"on\">New User</v-btn>\r\n            </template>\r\n            <v-card>\r\n              <v-card-title>\r\n                <span class=\"text-h5\">{{ formTitle }}</span>\r\n              </v-card-title>\r\n              <v-card-text>\r\n                <v-container>\r\n                  <v-row>\r\n                    <v-col cols=\"12\" sm=\"6\" >\r\n                      <v-text-field v-model=\"editedItem.username\" label=\"Username\"></v-text-field>\r\n                    </v-col>\r\n                    <v-col cols=\"12\" sm=\"6\" >\r\n                      <v-text-field v-model=\"editedItem.role\" label=\"Role\"></v-text-field>\r\n                    </v-col>\r\n                  </v-row>\r\n                </v-container>\r\n              </v-card-text>\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn color=\"blue darken-1\" text @click=\"close\">Cancel</v-btn>\r\n                <v-btn color=\"blue darken-1\" text @click=\"save(editedItem.id)\">Save</v-btn>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-dialog>\r\n          <!--DELETE DIALOG-->\r\n          <v-dialog v-model=\"dialogDelete\" max-width=\"600px\">\r\n            <v-card>\r\n              <v-card-title v-model=\"editedItem\" class=\"text-h5 justify-center\">Are you sure you want to remove user: {{editedItem.username}}?</v-card-title>\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <!--TODO: SEND REQUEST HERE BROTHER-->\r\n                <v-btn color=\"blue darken-1\" text @click=\"closeDelete\">Cancel</v-btn>\r\n                <v-btn color=\"blue darken-1\" text @click=\"deleteUser\">OK</v-btn>\r\n                <v-spacer></v-spacer>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-dialog>\r\n        </v-toolbar>\r\n      </template>\r\n      <template v-slot:item.actions=\"{ item }\">\r\n        <v-icon small class=\"mr-2\" @click=\"editUser(item)\" title=\"Edit User\">mdi-pencil</v-icon>\r\n        <v-icon small @click=\"confirmDeleteUser(item)\" title=\"Delete User\">mdi-delete</v-icon>\r\n      </template>\r\n    </v-data-table>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nimport api from \"@/service/apiService\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      dialog: false,\r\n      dialogDelete: false,\r\n      users: [],\r\n      search: \"\",\r\n      editedIndex: -1,\r\n      headers: [\r\n        {text: 'User Id', value: 'id'},\r\n        {text: 'Username', value: 'user'},\r\n        {text: 'Role', value: 'role'},\r\n        {text: 'Actions', value: 'actions', sortable: false},\r\n      ],\r\n      // TODO: FIX THIS LATER\r\n      editedItem: {\r\n        id: '',\r\n        username: '',\r\n        role: '',\r\n      },\r\n      defaultItem: {\r\n        id: '',\r\n        username: '',\r\n        role: '',\r\n      },\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    readUsers: async function () {\r\n      const data = await api.readUser();\r\n      this.users = data;\r\n      console.log(data)\r\n    },\r\n    editUser(user) {\r\n      this.editedIndex = this.users.indexOf(user)\r\n      this.editedItem = Object.assign({}, user)\r\n      this.dialog = true\r\n    },\r\n    confirmDeleteUser(item){\r\n      this.editedIndex = this.users.indexOf(item)\r\n      this.editedItem = Object.assign({}, item)\r\n      this.dialogDelete = true\r\n    },\r\n    deleteUser: async function() {\r\n      let response = await api.deleteUser(this.editedItem.id)\r\n      console.log(response)\r\n      window.location.reload();\r\n    },\r\n    close () {\r\n      this.dialog = false\r\n      this.$nextTick(() => {\r\n        this.editedItem = Object.assign({}, this.defaultItem)\r\n        this.editedIndex = -1\r\n      })\r\n    },\r\n    closeDelete () {\r\n      this.dialogDelete = false\r\n      this.$nextTick(() => {\r\n        this.editedItem = Object.assign({}, this.defaultItem)\r\n        this.editedIndex = -1\r\n      })\r\n    },\r\n    save: async function(id) {\r\n      if (this.editedIndex > -1) {\r\n        let response = await api.updateUser(this.editedItem,id)\r\n        console.log(response)\r\n        window.location.reload();\r\n      } else {\r\n        let response = await api.createUser(this.editedItem)\r\n        console.log(response)\r\n        window.location.reload();\r\n      }\r\n      this.close()\r\n    },\r\n  },\r\n  mounted() {\r\n    this.readUsers();\r\n  },\r\n  computed: {\r\n    formTitle () {\r\n      return this.editedIndex === -1 ? 'Create new user' : 'Edit user'\r\n    },\r\n  },\r\n  watch: {\r\n    dialog (val) {\r\n      val || this.close()\r\n    },\r\n    dialogDelete (val) {\r\n      val || this.closeDelete()\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/components"}]}